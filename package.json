{
  "name": "headline-creation-tool",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config ./bin/www",
    "dev": "npm run dev:build && npm run dev:serve && npm run dev:client",
    "dev:build": "webpack --mode development --progress",
    "dev:serve": "nodemon -r dotenv/config ./bin/www",
    "dev:client": "webpack-dev-server --mode development --port 3030 --open --inline --hot --content-base ./public",
    "build": "webpack --mode production --progress --hide-modules",
    "static": "npm run build && npm run dev:serve"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "^4.16.4",
    "mongoose": "^4.13.18",
    "nodemon": "^1.18.9",
    "serve-favicon": "~2.4.5",
    "trunks-log": "^2.0.3",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vuetify": "^1.4.4",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.5.22",
    "vuetify-loader": "^1.2.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "eslintConfig": {
    "root": true,
    "extends": "standard"
  }
}
